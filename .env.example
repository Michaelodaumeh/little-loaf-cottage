# Example environment variables for local development
# Copy this file to `.env` and fill in real values. Do NOT commit `.env`.

# Client (Vite) - these are embedded into the front-end at build time
VITE_SQUARE_APPLICATION_ID=your_vite_square_application_id_here
VITE_SQUARE_LOCATION_ID=your_vite_square_location_id_here
VITE_SQUARE_ENVIRONMENT=your_square_environment_here # e.g. sandbox or production

# Server (Netlify functions)
SQUARE_ACCESS_TOKEN=your_actual_square_access_token_here
SQUARE_LOCATION_ID=your_square_location_id_here
SQUARE_ENVIRONMENT=your_square_environment_here # e.g. sandbox or production
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=your_verified_sendgrid_sender@example.com
